{
  "name": "moodifyme/web-app",
  "description": "MoodifyMe - AI-powered mood detection and recommendation system",
  "type": "project",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "MoodifyMe\\": "includes/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php install.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
